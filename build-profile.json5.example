{
  "app": {
    "signingConfigs": [
      // 复制此文件为 build-profile.json5 并配置你自己的签名信息
      // Copy this file to build-profile.json5 and configure your own signing info
      // 
      // Debug 签名会由 DevEco Studio 自动生成
      // Debug signing will be auto-generated by DevEco Studio
      //
      // Release 签名需要在 AGC 控制台申请证书
      // Release signing requires certificate from AGC console
    ],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        },
        "targetSdkVersion": "6.0.2(22)"
      }
    ],
    "buildModeSet": [
      {
        "name": "debug"
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "nativelib",
      "srcPath": "./nativelib",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    }
  ]
}
